{
  "name": "raita",
  "version": "0.0.0",
  "description": "Dashboard for Cucumber test results",
  "repository": {
    "type": "git",
    "url": "https://git.wikimedia.org/git/integration/raita.git"
  },
  "scripts": {
    "doc": "jsduck",
    "watch": "riot -w src/ build/tags.js",
    "build": "riot src/ build/tags.js",
    "test": "jshint src/*.js"
  },
  "devDependencies": {
    "riot": "2.0.14",
    "jquery": "1.11.2",
    "jshint": "2.7.0"
  }
}
